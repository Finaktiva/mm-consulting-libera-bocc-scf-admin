        "@ngrx/store-devtools": "^8.1.0",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "@nrwl/angular": "8.8.1",
        "aws-amplify": "^1.1.25",
        "aws-amplify-angular": "^2.1.12",
        "bn-ng-idle": "1.0.1",
        "core-js": "^2.6.3",
        "immer": "^3.1.2",
        "invert-color": "^2.0.0",
        "material-palette": "^1.0.0",
        "moment": "^2.24.0",
        "ngx-file-dropzone": "0.0.1",
        "ngx-translate-multi-http-loader": "^3.0.0",
        "rxjs": "^6.5.3",
        "rxjs-augmented": "^1.0.1",
        "sweetalert2": "^11.4.10",
        "tslib": "^2.4.0",
        "web-animations-js": "github:angular/web-animations-js#release_pr208",
        "whatwg-fetch": "^3.0.0",
        "xlsx": "^0.18.5",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "^8.0.1",
        "@angular-builders/dev-server": "^7.3.1",
        "@angular-devkit/build-angular": "^0.803.3",
        "@angular/cli": "8.3.19",
        "@angular/compiler-cli": "^8.2.14",
        "@angular/language-service": "^8.2.14",
        "@nrwl/cypress": "8.8.1",
        "@nrwl/workspace": "8.8.1",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/jquery": "3.3.6",
        "@types/node": "~8.9.4",
