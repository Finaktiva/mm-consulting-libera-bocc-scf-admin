{
    "name": "libera",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "mock": "ng serve -c=mock",
        "build": "ng build",
        "test": "ng test",
        "lint": "./node_modules/.bin/nx workspace-lint && ng lint",
        "e2e": "ng e2e",
        "build:stats": "ng build --prod --stats-json",
        "analyze": "webpack-bundle-analyzer dist/apps/landing/stats-es2015.json",
        "start:admin": "ng serve admin -o --port=4201",
        "start:admin:mock": "ng serve admin -o -c=mock --port=4201",
        "build:admin:stats": "ng build admin --stats-json --prod",
        "analyze:admin": "webpack-bundle-analyzer dist/apps/admin/stats-es2015.json",
        "start:enterprise": "ng serve enterprise -o --port=4202",
        "start:enterprise:mock": "ng serve enterprise -o -c=mock --port=4202",
        "build:enterprise": "ng build enterprise --prod",
        "build:enterprise:stats": "ng build enterprise --prod --stats-json",
        "analyze:enterprise": "webpack-bundle-analyzer dist/apps/enterprise/stats-es2015.json",
        "affected:apps": "./node_modules/.bin/nx affected:apps",
        "affected:libs": "./node_modules/.bin/nx affected:libs",
        "affected:build": "./node_modules/.bin/nx affected:build",
        "affected:e2e": "./node_modules/.bin/nx affected:e2e",
        "affected:test": "./node_modules/.bin/nx affected:test",
        "affected:lint": "./node_modules/.bin/nx affected:lint",
        "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
        "affected": "./node_modules/.bin/nx affected",
        "format": "./node_modules/.bin/nx format:write",
        "format:write": "./node_modules/.bin/nx format:write",
        "format:check": "./node_modules/.bin/nx format:check",
        "update": "ng update @nrwl/workspace",
        "update:check": "ng update",
        "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
        "schematic:build": "tsc -p tools/tsconfig.tools.json",
        "dep-graph": "./node_modules/.bin/nx dep-graph",
        "help": "./node_modules/.bin/nx help",
        "nx": "nx",
        "start:prod": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng serve enterprise",
        "build:admin:dev": "node --max_old_space_size=8400 node_modules/@angular/cli/bin/ng build admin --c=dev --aot --build-optimizer  --optimization=true --outputHashing=all --baseHref=/admin --deploy-url=admin/",
        "build:enterprise:dev": "node --max_old_space_size=8400 node_modules/@angular/cli/bin/ng build enterprise --c=dev --aot --build-optimizer --optimization=true --outputHashing=all --baseHref=/enterprise --deploy-url=enterprise/",
        "build:landing:dev": "node --max_old_space_size=6144 node_modules/@angular/cli/bin/ng build landing --c=dev --aot --build-optimizer --optimization=true --outputHashing=all",
        "build:admin:stage": "node --max_old_space_size=8400 node_modules/@angular/cli/bin/ng build admin --c=stage --aot --build-optimizer  --optimization=true --outputHashing=all --baseHref=/admin --deploy-url=admin/",
        "build:enterprise:stage": "node --max_old_space_size=8400 node_modules/@angular/cli/bin/ng build enterprise --c=stage --aot --build-optimizer --optimization=true --outputHashing=all --baseHref=/enterprise --deploy-url=enterprise/",
        "build:landing:stage": "node --max_old_space_size=6144 node_modules/@angular/cli/bin/ng build landing --c=stage --aot --build-optimizer --optimization=true --outputHashing=all",
        "build:admin:prod": "node --max_old_space_size=8400 node_modules/@angular/cli/bin/ng build admin --c=production --aot --build-optimizer  --optimization=true --outputHashing=all --baseHref=/admin --deploy-url=admin/",
        "build:enterprise:prod": "node --max_old_space_size=8400 node_modules/@angular/cli/bin/ng build enterprise --c=production --aot --build-optimizer --optimization=true --outputHashing=all --baseHref=/enterprise --deploy-url=enterprise/",
        "build:landing:prod": "node --max_old_space_size=6144 node_modules/@angular/cli/bin/ng build landing --c=production --aot --build-optimizer --optimization=true --outputHashing=all"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^8.2.14",
        "@angular/cdk": "^8.2.3",
        "@angular/common": "^8.2.14",
